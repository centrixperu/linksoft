				<li class="tab"><a href="#tab_8" class='waves-effect waves-light'>Tab 8</a></li>
				<li class="tab"><a href="#tab_9" class='waves-effect waves-light'>Tab 9</a></li>
				<li class="tab"><a href="#tab_10" class='waves-effect waves-light'>Tab 10</a></li>
				<li class="tab"><a href="#tab_lista" class='waves-effect waves-light'>Lista</a></li>
			</ul>
		</div>
		
		<div class="row">

			<div id="tab_lista" class="col s12 st_lks container_ft_1 height_-129px" data-col="scroll_tab_lista" data-col_2="div_width_-5px">

				<div class="col s12 sub_container full_height">

					<div class="col s12 container_table footer_fixed">

						<section class='ft_1'>
							<div class="row">
								<div class="col s4 m4 l3 xl2 hide_for_600px" data-col='c_s_grid_lista_filtros'>
									<div class='col_label'>Filtrar por</div>
								</div>
								<div class="col s9 m6 l4 xl4">
									<div class='col_label'>Valor</div>
									<input class='lks' placeholder="Digite valor a buscar" value="" type="text" data-col="i_filter_grid_lista" />
								</div>
								<div class="col s3 m2 l5 xl6">
									<div class='col_label'> </div>
									<div data-col="c_length_grid_lista"></div>
								</div>
							</div>
						</section>
						
						<div class="content_table" data-col='scroll_grid_lista' data-col_2="p-right-15">
							<div class="row">
								<div class="c_col col s12" data-col="c_grid_lista"></div>
							</div>
						</div>

						<div class="dataTables_wrapper" data-col="c_footer_grid_lista"></div>
						
					</div>

				</div>

			</div>

			@RenderBody()

		</div>
    </div>

    @Scripts.Render("~/bundlesafter/lib")
    @RenderSection("scripts", required: false)
    @RenderSection("Styles", false)

	<script>fc__frm_layout__tipo = 'Master';</script>
