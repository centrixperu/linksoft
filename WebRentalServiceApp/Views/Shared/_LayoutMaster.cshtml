				<div class="side six"></div>
			</div>
		</div>
	</div>
	<!-- <div id="contenedor_carga"><div class="js-loading-indicator triple"></div></div> -->

    <div class="container">
	
		<div class="row ft_tab_1 ft_tab_master_fixed shadow-01">
			<ul id="tab_main" class="tabs">
				<li class="tab"><a href="#tab_1" class='waves-effect waves-light'>Tab 1</a></li>
				<li class="tab"><a href="#tab_2" class='waves-effect waves-light'>Tab 2</a></li>
				<li class="tab"><a href="#tab_3" class='waves-effect waves-light'>Tab 3</a></li>
				<li class="tab"><a href="#tab_4" class='waves-effect waves-light'>Tab 4</a></li>
				<li class="tab"><a href="#tab_5" class='waves-effect waves-light'>Tab 5</a></li>
