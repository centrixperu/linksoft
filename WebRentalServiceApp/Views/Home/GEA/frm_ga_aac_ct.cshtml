        <div class="modal_header_inner">
            <h3 class='m-all-0'>Año Académico</h3>
        </div>
    </header>

	<form id="form_main" class="height_-69px" onsubmit='return false;'>
		<div class='modal-content st_lks height_-50px'>
		
				<div class="col s12 sub_container full_height">
				
					<div class="col s12 full_height">
					

						<!-- <div class="modal-sub-content p-right-15 div_width_15px" data-col="scroll_div_mdl_grid_it_1" data-col_2="div_width_10px"> -->
						
						<div class="modal-sub-content height_-14px" data-col="scroll_div_mdl_grid_it_1" data-col_2="div_width_10px" style='margin-top: 0px !important;'>
						
							<section>
								
								<div class="row" style='height: 47px;'>
									<div class="col s12" style='height: 20px;'>
										<div class='col_label_head'>Datos generales</div>
									</div>
									<div class='lks switch'>
										<label>
											<span>Estado</span>
											<input type='checkbox' class='show_cest' data-col='cest' data-hb_v='n' data-hb_n='s' data-hb_e='s' />
